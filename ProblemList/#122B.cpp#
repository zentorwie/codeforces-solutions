#include <list>
#include <cstdlib>
#include <cstring>
#include <ctime>
#include <cstdio>
#include <cmath>
using namespace std;

int main()
{
    string ss;
    cin >> ss;
    int n[11] = {0};
    int f = 0, s = 0;
    int l = ss.length();
    
    
    for (int i = 0; i < l; i++) {
        n[ss[i] - '0']++;
    }
    
    int maxi = 0;
    for (int i = 1; i <= 9; i++) {
        if (n[i] > n[maxi])
            maxi = i;
    }
    
    if (maxi == 4 || maxi == 7) {
        if (n[4] == n[7]) printf("4\n");
        else if (n[4] > n[7]) printf("4\n
        
    }
    else 
        printf("-1\n");
    
    return 0;
}
