#include <stdio.h>
#include <string.h>
int main()
{
  int n, m;
  scanf("%d%d", n, m);
  
  int m[110][110];
  
  int full=0, half=0, zero=0;
  char str[5];
  
  for (int i = 0; i < n; i++)
    for (int j = 0; j < m; j++) {
      scanf("%s", str);
      if (str[0] == '1' && str[1] == '1')
	full++;
      else if (str[0] == '0' && str[1] == '0')
	zero++;
      else
	half++;
    }
  

  
  return 0;
}
