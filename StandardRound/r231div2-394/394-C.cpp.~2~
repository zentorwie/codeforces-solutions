#include <stdio.h>
#include <string.h>
int main()
{
  int n, m;
  scanf("%d%d", &n, &m);
  
  int A[110][110];
  
  int full=0, half=0, zero=0;
  char str[5];
  
  for (int i = 0; i < n; i++)
    for (int j = 0; j < m; j++) {
      scanf("%s", str);

      if (str[0] == '1' && str[1] == '1' && )
	full++;
      else if (str[0] == '0' && str[1] == '0')
	zero++;
      else
	half++;
      
    }
//  printf("%d %d %d\n", full, half, zero);
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (half) {
	A[i][j] = 1;
	half--;
      }
      else if (full) {
	A[i][j] = 2;
	full--;
      }
      else
	break;
    }
  }
  


  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m-1; j++) {
      if (A[i][j] == 2)
	printf("11 ");
      else if (A[i][j] == 1 && i % 2 == 0)
	printf("01 ");
      else if (A[i][j] == 1 && i % 2 == 1)
	printf("10 ");
      else
	printf("00 ");
    }
    if (A[i][m-1] == 2)
      printf("11");
    else if (A[i][m-1] == 1 && i % 2 == 0)
      printf("01");
    else if (A[i][m-1] == 1 && i % 2 == 1)
      printf("10");
    else
      printf("00");
    printf("\n");
  }
  
  return 0;
}
